  "query": "\n                SELECT \n                    `id`, `uuid`, `w`.`user_id`, `value`, `is_regex`,\n                    GROUP_CONCAT(`word_excluded_users`.`user_id` SEPARATOR ',') AS `excluded_user_ids`\n                FROM \n                    `word_subscriptions` w\n                LEFT JOIN\n                    `word_excluded_users`\n                ON\n                    `w`.`id` = `word_excluded_users`.`subs_id`\n                GROUP BY\n                    `w`.`id`\n            ",
